<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="maksym_campaign_campaign" resource="default" engine="innodb" comment="maksym_campaign_campaign Table">
		<column xsi:type="int" name="campaign_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="campaign_id"/>
		</constraint>
		<column name="active" nullable="true" xsi:type="boolean" comment="active"/>
		<column name="title" nullable="true" xsi:type="text" comment="title"/>
		<column name="description" nullable="true" xsi:type="text" comment="description"/>
		<column name="uri" nullable="true" xsi:type="text" comment="uri"/>
		<column name="products" nullable="true" xsi:type="text" comment="products"/>
	</table>
</schema>
